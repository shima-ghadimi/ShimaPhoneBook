@model List<PhoneBook.Domain.Core.People.Person>



@if (Model != null && Model.Count > 0)
{
<table class="table table-bordered table-striped">
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <td>Detail</td>
    </tr>
    @foreach (var item in Model)
    {
<tr>
    <td>@item.FirstName</td>
    <td>@item.LastName</td>
    <td>@item.Email</td>
    <td><a asp-controller="People" asp-action="Detail" asp-route-id="@item.Id">Detail</a></td>
</tr>}
</table>}